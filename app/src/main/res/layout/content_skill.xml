<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_skill"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@color/colorAccent"
     app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ninestack.kodeworks.SkillActivity"
    tools:showIn="@layout/activity_skill">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:weightSum="2">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/htmlcss"
            android:id="@+id/imageView3"
            android:paddingRight="10dp"
            android:layout_weight="1" />

        <com.eralp.circleprogressview.CircleProgressView
            android:id="@+id/circle_progress_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="80dp"
            android:layout_gravity="center"
            app:cpv_circle_color="#27375c"
            app:cpv_circle_width="6dp"
            app:cpv_background_circle_width="1dp"
            app:cpv_background_circle_color="#5b253048"
            app:cpv_text_prefix="%"
            app:cpv_text_size="30"

            app:cpv_text_color="#27375c"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:weightSum="2">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/wordpresslogo"
            android:id="@+id/imageView5"
            android:paddingRight="10dp"
            android:layout_weight="1" />

        <com.eralp.circleprogressview.CircleProgressView
            android:id="@+id/circle_progress_view1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="80dp"
            android:layout_gravity="center"
            app:cpv_circle_color="#64dd17"
            app:cpv_circle_width="8dp"
            app:cpv_background_circle_width="1dp"
            app:cpv_background_circle_color="#5b253048"
            app:cpv_text_prefix="%"
            app:cpv_text_size="30"
            app:cpv_text_color="#27375c"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:weightSum="2">


        <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/woocommercelogo"
            android:layout_weight="1"

            android:paddingRight="10dp"
             android:id="@+id/imageView6" />


        <com.eralp.circleprogressview.CircleProgressView
            android:id="@+id/circle_progress_view2"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="80dp"
            android:layout_gravity="center"
            app:cpv_circle_color="#e57373"
            app:cpv_circle_width="6dp"
            app:cpv_background_circle_width="1dp"
            app:cpv_background_circle_color="#5b253048"
            app:cpv_text_prefix="%"
            app:cpv_text_size="30"
            app:cpv_text_color="#27375c"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:weightSum="2">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/jquerylogo"
            android:id="@+id/imageView7"
            android:paddingRight="10dp"
            android:layout_weight="1" />

        <com.eralp.circleprogressview.CircleProgressView
            android:id="@+id/circle_progress_view3"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="80dp"
            android:layout_gravity="center"
            app:cpv_circle_color="#5c6bc0"
            app:cpv_circle_width="6dp"
            app:cpv_background_circle_width="1dp"
            app:cpv_background_circle_color="#5b253048"
            app:cpv_text_prefix="%"
            app:cpv_text_size="30"
            app:cpv_text_color="#27375c"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:weightSum="2">

  <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/php_logo"

            android:id="@+id/imageView8"
      android:paddingRight="10dp"
            android:layout_weight="1" />
        <com.eralp.circleprogressview.CircleProgressView
            android:id="@+id/circle_progress_view4"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="80dp"
            android:layout_gravity="center"
            app:cpv_circle_color="#ec407a"
            app:cpv_circle_width="6dp"
            app:cpv_background_circle_width="1dp"
            app:cpv_background_circle_color="#5b253048"
            app:cpv_text_prefix="%"
            app:cpv_text_size="30"
            app:cpv_text_color="#27375c"/>
</LinearLayout>

    </LinearLayout>
</LinearLayout>
